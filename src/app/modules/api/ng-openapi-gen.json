{
  "$schema": "./node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "http://localhost:5001/swagger/v1/swagger.json",
  "output": "./src/app/modules/api/ng-openapi",
  "prefix": "api",
  "includeTags": [],
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "sortParams": "desc",
  "defaultTag": "ApiGe",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": true,
  "generateExamples": false,
  "camelCase": true,
  "customFileSuffix": {
    "model": "",
    "example": ".example",
    "service": ".service"
  },
  "timeout": 20000,
  "skipProxySetup": false
}
